<div class="row">
	<div class="col-md-8 col-md-offset-2">
		<h1>User Profile</h1>
		<hr>
		<h1>My Orders</h1>
		{{# each orders}}
		<div class="panel panel-default">
		  	<div class="panel-body">
		    	<ul class="list-group">
		    	{{# each this.items}}
				  	<li class="list-group-item">
				    	<span class="badge">{{this.price}}</span>
				    	<a href='#'>{{this.item.title}}</a> | {{this.qty}}
				  	</li>
				{{/ each}}
				</ul>
		  	</div>
		  	<div class="panel-footer">Total Price:$ {{ this.cart.totalPrice}}</div>
		</div>
		{{/each}}
	</div>
</div>